{
  "$schema": "vscode://schemas/color-theme",
  "name": "Blue PSL 10K",
  "type": "light",
  "colors": {
    "editor.background": "#eff1f5",
    "editor.foreground": "#4c4f69",
    "editor.lineHighlightBackground": "#4c4f6910",
    "editor.selectionBackground": "#a9c1e2",
    "editor.selectionForeground": "#24292f",
    "editorCursor.foreground": "#dc8a78",
    "editorLineNumber.foreground": "#8c8fa1",
    "editorLineNumber.activeForeground": "#7287fd",

    "activityBar.background": "#e6e9ef",
    "activityBar.foreground": "#3c3f54",
    "activityBar.inactiveForeground": "#6c6f85",
    "activityBar.activeBorder": "#3465a4",
    "activityBar.activeBackground": "#eff1f5",
    "activityBar.dropBorder": "#3465a4",
    "activityBarBadge.background": "#3465a4",
    "activityBarBadge.foreground": "#eff1f5",

    "sideBar.background": "#e6e9ef",
    "sideBar.foreground": "#5c5f77",
    "sideBar.dropBackground": "#ccd0da80",
    "sideBarTitle.foreground": "#5c5f77",
    "sideBarSectionHeader.background": "#e6e9ef",
    "sideBarSectionHeader.foreground": "#5c5f77",
    "sideBarSectionHeader.border": "#ccd0da",

    "statusBar.background": "#3465a4",
    "statusBar.foreground": "#eff1f5",
    "statusBar.border": "#2855a3",
    "statusBar.debuggingBackground": "#fe640b",
    "statusBar.debuggingForeground": "#eff1f5",
    "statusBar.debuggingBorder": "#fe640b",
    "statusBar.noFolderBackground": "#3465a4",
    "statusBar.noFolderForeground": "#eff1f5",
    "statusBar.noFolderBorder": "#2855a3",
    "statusBarItem.remoteBackground": "#3465a4",
    "statusBarItem.remoteForeground": "#eff1f5",
    "statusBarItem.remoteHoverBackground": "#5a85c2",
    "statusBarItem.remoteHoverForeground": "#eff1f5",
    "statusBarItem.hoverBackground": "#5a85c2",
    "statusBarItem.activeBackground": "#2855a3",
    "statusBarItem.prominentBackground": "#ccd0da",
    "statusBarItem.prominentForeground": "#4c4f69",
    "statusBarItem.prominentHoverBackground": "#eff1f5",
    "statusBarItem.errorBackground": "#d20f39",
    "statusBarItem.errorForeground": "#eff1f5",
    "statusBarItem.errorHoverBackground": "#e6475f",
    "statusBarItem.errorHoverForeground": "#eff1f5",
    "statusBarItem.warningBackground": "#df8e1d",
    "statusBarItem.warningForeground": "#eff1f5",
    "statusBarItem.warningHoverBackground": "#e9a84d",
    "statusBarItem.warningHoverForeground": "#eff1f5",

    "titleBar.activeBackground": "#e6e9ef",
    "titleBar.activeForeground": "#4c4f69",
    "titleBar.inactiveBackground": "#eff1f5",
    "titleBar.inactiveForeground": "#6c6f85",

    "tab.activeBackground": "#eff1f5",
    "tab.activeForeground": "#4c4f69",
    "tab.inactiveBackground": "#e6e9ef",
    "tab.inactiveForeground": "#6c6f85",
    "tab.border": "#ccd0da",
    "tab.hoverBackground": "#e6e9ef",
    "tab.hoverForeground": "#4c4f69",

    "editorGroupHeader.tabsBackground": "#e6e9ef",
    "editorGroupHeader.noTabsBackground": "#e6e9ef",
    "editorGroupHeader.border": "#ccd0da",

    "panel.background": "#e6e9ef",
    "panel.border": "#ccd0da",
    "panelTitle.activeBorder": "#3465a4",
    "panelTitle.activeForeground": "#4c4f69",
    "panelTitle.inactiveForeground": "#6c6f85",

    "terminal.background": "#eff1f5",
    "terminal.foreground": "#4c4f69",
    "terminal.ansiBlack": "#5c5f77",
    "terminal.ansiBlue": "#3465a4",
    "terminal.ansiCyan": "#179299",
    "terminal.ansiGreen": "#40a02b",
    "terminal.ansiMagenta": "#ea76cb",
    "terminal.ansiRed": "#d20f39",
    "terminal.ansiWhite": "#acb0be",
    "terminal.ansiYellow": "#df8e1d",
    "terminal.ansiBrightBlack": "#6c6f85",
    "terminal.ansiBrightBlue": "#3465a4",
    "terminal.ansiBrightCyan": "#179299",
    "terminal.ansiBrightGreen": "#40a02b",
    "terminal.ansiBrightMagenta": "#ea76cb",
    "terminal.ansiBrightRed": "#d20f39",
    "terminal.ansiBrightWhite": "#bcc0cc",
    "terminal.ansiBrightYellow": "#df8e1d",

    "button.background": "#3465a4",
    "button.foreground": "#eff1f5",
    "button.hoverBackground": "#2855a3",

    "focusBorder": "#3465a4",
    "icon.foreground": "#3465a4",
    "input.background": "#eff1f5",
    "input.border": "#ccd0da",
    "input.foreground": "#4c4f69",
    "input.placeholderForeground": "#9ca0b0",

    "list.activeSelectionBackground": "#3465a4",
    "list.activeSelectionForeground": "#eff1f5",
    "list.activeSelectionIconForeground": "#eff1f5",
    "list.hoverBackground": "#eff1f5",
    "list.hoverForeground": "#4c4f69",
    "list.focusBackground": "#dce0e8",
    "list.focusForeground": "#4c4f69",
    "list.focusHighlightForeground": "#eff1f5",
    "list.inactiveSelectionBackground": "#ccd0da",
    "list.inactiveSelectionForeground": "#4c4f69",
    "list.highlightForeground": "#3465a4",

    "quickInput.background": "#e6e9ef",
    "quickInput.foreground": "#4c4f69",
    "quickInputList.focusBackground": "#3465a4",
    "quickInputList.focusForeground": "#eff1f5",
    "quickInputList.focusIconForeground": "#eff1f5",
    "quickInputTitle.background": "#e6e9ef",

    "pickerGroup.border": "#ccd0da",
    "pickerGroup.foreground": "#3465a4",

    "gitDecoration.addedResourceForeground": "#40a02b",
    "gitDecoration.modifiedResourceForeground": "#df8e1d",
    "gitDecoration.deletedResourceForeground": "#d20f39",
    "gitDecoration.untrackedResourceForeground": "#40a02b",
    "gitDecoration.conflictingResourceForeground": "#e64553",

    "editorError.foreground": "#d20f39",
    "editorWarning.foreground": "#df8e1d",
    "editorInfo.foreground": "#179299",

    "diffEditor.insertedTextBackground": "#40a02b20",
    "diffEditor.removedTextBackground": "#d20f3920"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7c7f93",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#40a02b"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#fe640b"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#8839ef"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#3465a4"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.class"],
      "settings": {
        "foreground": "#df8e1d"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#4c4f69"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#e64553"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#8839ef"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#df8e1d"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#04a5e5"
      }
    },
    {
      "scope": ["punctuation.definition.string", "string.regexp"],
      "settings": {
        "foreground": "#ea76cb"
      }
    },
    {
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": {
        "foreground": "#3465a4"
      }
    },
    {
      "scope": ["entity.name.function.macro", "meta.preprocessor"],
      "settings": {
        "foreground": "#dc8a78"
      }
    },
    {
      "name": "Markdown body text",
      "scope": ["text.html.markdown", "meta.paragraph.markdown"],
      "settings": {
        "foreground": "#3465a4"
      }
    },
    {
      "name": "Markdown headings",
      "scope": ["markup.heading", "punctuation.definition.heading.markdown"],
      "settings": {
        "foreground": "#8839ef",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown inline code",
      "scope": ["markup.inline.raw", "markup.raw.inline.markdown"],
      "settings": {
        "foreground": "#fe640b"
      }
    },
    {
      "name": "Markdown links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#1e66f5"
      }
    },
    {
      "name": "Markdown link text",
      "scope": [
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#179299"
      }
    },
    {
      "name": "Markdown blockquote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#7c7f93",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown list markers",
      "scope": ["punctuation.definition.list.begin.markdown"],
      "settings": {
        "foreground": "#df8e1d"
      }
    },
    {
      "name": "Markdown fenced code block language",
      "scope": ["fenced_code.block.language.markdown"],
      "settings": {
        "foreground": "#40a02b"
      }
    },
    {
      "name": "YAML keys",
      "scope": [
        "entity.name.tag.yaml",
        "punctuation.definition.block.sequence.item.yaml"
      ],
      "settings": {
        "foreground": "#3465a4"
      }
    },
    {
      "name": "YAML anchors and aliases",
      "scope": ["entity.name.type.anchor.yaml", "variable.other.alias.yaml"],
      "settings": {
        "foreground": "#df8e1d"
      }
    },
    {
      "name": "YAML types and tags",
      "scope": ["storage.type.tag-handle.yaml", "constant.language.yaml"],
      "settings": {
        "foreground": "#fe640b"
      }
    },
    {
      "name": "CSS selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#8839ef"
      }
    },
    {
      "name": "CSS properties",
      "scope": ["support.type.property-name.css", "meta.property-name.css"],
      "settings": {
        "foreground": "#3465a4"
      }
    },
    {
      "name": "CSS values and units",
      "scope": [
        "support.constant.property-value.css",
        "keyword.other.unit.css",
        "constant.other.color.css"
      ],
      "settings": {
        "foreground": "#fe640b"
      }
    },
    {
      "name": "CSS pseudo-classes and pseudo-elements",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#179299"
      }
    },
    {
      "name": "SCSS/SASS variables",
      "scope": ["variable.scss", "variable.sass", "variable.css"],
      "settings": {
        "foreground": "#e64553"
      }
    },
    {
      "name": "SCSS mixins and functions",
      "scope": [
        "entity.name.function.scss",
        "keyword.control.at-rule.mixin.scss",
        "keyword.control.at-rule.include.scss"
      ],
      "settings": {
        "foreground": "#3465a4"
      }
    },
    {
      "name": "Shell variables",
      "scope": [
        "variable.other.normal.shell",
        "variable.other.special.shell",
        "variable.other.positional.shell",
        "variable.other.bracket.shell",
        "punctuation.definition.variable.shell"
      ],
      "settings": {
        "foreground": "#e64553"
      }
    },
    {
      "name": "Shell builtins and commands",
      "scope": ["support.function.builtin.shell", "entity.name.command.shell"],
      "settings": {
        "foreground": "#3465a4"
      }
    },
    {
      "name": "Shell shebang",
      "scope": [
        "comment.line.shebang",
        "punctuation.definition.comment.shebang"
      ],
      "settings": {
        "foreground": "#dc8a78",
        "fontStyle": ""
      }
    },
    {
      "name": "TOML section headers",
      "scope": [
        "entity.name.section.toml",
        "punctuation.definition.table.toml",
        "punctuation.definition.table.array.toml"
      ],
      "settings": {
        "foreground": "#8839ef",
        "fontStyle": "bold"
      }
    },
    {
      "name": "TOML keys",
      "scope": ["support.type.property-name.toml", "variable.key.toml"],
      "settings": {
        "foreground": "#3465a4"
      }
    },
    {
      "name": "TOML dates and times",
      "scope": ["constant.other.date.toml", "constant.other.datetime.toml"],
      "settings": {
        "foreground": "#179299"
      }
    },
    {
      "name": "Diff inserted",
      "scope": ["markup.inserted.diff", "punctuation.definition.inserted.diff"],
      "settings": {
        "foreground": "#40a02b"
      }
    },
    {
      "name": "Diff deleted",
      "scope": ["markup.deleted.diff", "punctuation.definition.deleted.diff"],
      "settings": {
        "foreground": "#d20f39"
      }
    },
    {
      "name": "Diff changed",
      "scope": ["markup.changed.diff"],
      "settings": {
        "foreground": "#df8e1d"
      }
    },
    {
      "name": "Diff header and range",
      "scope": [
        "meta.diff.header",
        "meta.diff.range",
        "punctuation.definition.range.diff"
      ],
      "settings": {
        "foreground": "#7287fd"
      }
    },
    {
      "name": "XML namespaces",
      "scope": ["entity.other.attribute-name.namespace.xml"],
      "settings": {
        "foreground": "#df8e1d"
      }
    },
    {
      "name": "XML DOCTYPE and entities",
      "scope": [
        "variable.language.documentroot.xml",
        "constant.character.entity.xml",
        "punctuation.definition.entity.xml"
      ],
      "settings": {
        "foreground": "#fe640b"
      }
    },
    {
      "name": "INI section headers",
      "scope": ["entity.name.section.group-title.ini"],
      "settings": {
        "foreground": "#8839ef",
        "fontStyle": "bold"
      }
    },
    {
      "name": "INI keys",
      "scope": ["keyword.other.definition.ini"],
      "settings": {
        "foreground": "#3465a4"
      }
    },
    {
      "name": "Makefile targets",
      "scope": ["entity.name.function.target.makefile"],
      "settings": {
        "foreground": "#3465a4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Makefile variables",
      "scope": ["variable.other.makefile", "variable.language.makefile"],
      "settings": {
        "foreground": "#e64553"
      }
    },
    {
      "name": "Makefile automatic variables",
      "scope": ["variable.language.automatic.makefile"],
      "settings": {
        "foreground": "#dc8a78"
      }
    },
    {
      "name": "reStructuredText headings",
      "scope": [
        "markup.heading.restructuredtext",
        "punctuation.definition.heading.restructuredtext"
      ],
      "settings": {
        "foreground": "#8839ef",
        "fontStyle": "bold"
      }
    },
    {
      "name": "reStructuredText directives",
      "scope": [
        "meta.directive.restructuredtext",
        "punctuation.definition.directive.restructuredtext"
      ],
      "settings": {
        "foreground": "#3465a4"
      }
    },
    {
      "name": "reStructuredText roles",
      "scope": ["string.other.interpreted.restructuredtext"],
      "settings": {
        "foreground": "#179299"
      }
    },
    {
      "name": "Punctuation brackets",
      "scope": [
        "punctuation.definition.block.begin",
        "punctuation.definition.block.end",
        "punctuation.brackets",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#6c6f85"
      }
    },
    {
      "name": "Punctuation separators",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#6c6f85"
      }
    },
    {
      "name": "Escape characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#ea76cb"
      }
    },
    {
      "name": "Invalid/deprecated",
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#d20f39",
        "fontStyle": "strikethrough"
      }
    }
  ]
}
